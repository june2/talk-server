{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=production tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "NODE_ENV=development nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "NODE_ENV=test jest --detectOpenHandles --config ./jest.json",
    "test:watch": "NODE_ENV=test jest --watch --config ./jest.json",
    "test:cov": "NODE_ENV=test jest --coverage",
    "test:debug": "NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest  --watch --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "6.1.1",
    "@nestjs/core": "6.1.1",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/mongoose": "^5.2.2",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "6.1.1",
    "@nestjs/swagger": "3.0.2",
    "aws-sdk": "^2.540.0",
    "class-transformer": "0.2.0",
    "class-validator": "0.9.1",
    "dotenv": "^8.0.0",
    "fcm-node": "^1.5.2",
    "joi": "^14.3.1",
    "moment": "^2.24.0",
    "mongoose": "5.5.2",
    "mongoose-aggregate-paginate-v2": "^1.0.2",
    "mongoose-paginate-v2": "^1.3.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "multer-sharp-s3": "^0.1.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.4.0",
    "swagger-ui-express": "^4.0.2"
  },
  "devDependencies": {
    "@nestjs/testing": "6.0.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.13",
    "@types/joi": "^14.3.3",
    "@types/mongoose": "5.3.26",
    "@types/node": "^10.12.18",
    "@types/supertest": "^2.0.7",
    "jest": "^23.6.0",
    "jest-cli": "^24.7.1",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "supertest": "^3.4.1",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "5.12.1",
    "typescript": "3.4.4"
  }
}
